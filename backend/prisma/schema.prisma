generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id       String   @id @unique @default(cuid())
  email    String   @unique
  password String
  images   images[]
}

model images {
  id String @id @unique @default(cuid())
  user   user?   @relation(fields: [userId], references: [id])
  userId String? @map("userid")
  path String?
  status String?
  timestamp String?
  jobId String?
  base64String String?
}
